# Cylestio UI Mock Environment Configuration

# API Configuration
API_BASE_URL=http://localhost:3001
MOCK_API_PORT=3001

# Database Configuration
DATABASE_PATH=./cylestio-mock.db
USE_MOCK_DATA=true

# Authentication Settings (Disabled in mock mode)
AUTH_ENABLED=false
AUTH_SECRET=mock-secret-not-for-production

# Features Flags
ENABLE_ADVANCED_ANALYTICS=true
ENABLE_AGENT_MANAGEMENT=true

# Logging
LOG_LEVEL=debug

# Performance Settings
CACHE_TTL=60

# Demo Mode
DEMO_MODE=true 