# Application
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# Database
DATABASE_URL=sqlite://./data/cylestio.db

# Authentication (replace with your preferred auth provider)
AUTH_SECRET=your-auth-secret
NEXTAUTH_URL=http://localhost:3000

# API Keys (replace with your actual keys)
CYLESTIO_MONITOR_API_KEY=your-monitor-api-key

# Monitoring and Analytics
NEXT_PUBLIC_ANALYTICS_ID=your-analytics-id

# Security
SECURITY_HEADERS_ENABLED=true
CSP_ENABLED=true

# Feature Flags
FEATURE_FLAG_NEW_DASHBOARD=true
FEATURE_FLAG_BETA_FEATURES=false

# Deployment
VERCEL_TOKEN=your-vercel-token
VERCEL_ORG_ID=your-org-id
VERCEL_PROJECT_ID=your-project-id

# Testing
PLAYWRIGHT_TEST_BASE_URL=http://localhost:3000 

# Descope
NEXT_PUBLIC_DESCOPE_PROJECT_ID=